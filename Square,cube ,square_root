def square (num):
    return num*num
def cube (num):
    return num*num*num 
def square_root (num):
    return num**0.5
number = int(input("Enter an integer number: "))
print("Square of ",number," is ", square(number))
print("Cube of ",number," is ", cube(number))
print("square_root",number,"is",square_root(number))
